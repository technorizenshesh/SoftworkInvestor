<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softworkinvestor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SoftworkInvestor">
        <activity android:name=".activities.HomeAct"></activity>
        <activity android:name=".activities.ChooseActivity" />
        <activity android:name=".activities.LoginAct" />
        <activity android:name=".activities.VerifyActivity" />
        <activity android:name=".activities.SignupActivity" />
        <activity android:name=".activities.WelcomeActivity" />
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>